{
  "extends": ["eslint:recommended", "eslint-config-prettier"],
  "env": {
    "node": true,
    "browser": false
  },
  "parserOptions": {
    "ecmaVersion": 13
  },
  "rules": {
    "no-unused-vars": "error",
    "semi": ["error", "always"],
    "quotes": ["error", "single"],
    "no-trailing-spaces": "error"
  }
}
